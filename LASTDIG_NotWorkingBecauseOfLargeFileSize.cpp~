#include<iostream>
using namespace std;
int main()
{
	unsigned long long int b;
	int a,t,i,lastdigit,c;
	cin>>t;
	for(i=0;i<t;i++)
	{
	cin>>a>>b;
	lastdigit=a%10;
	c=b%100;
	switch(lastdigit)
	{
		case 0:cout<<"0\n";break;
		case 6:cout<<"6\n";break;
		case 5:cout<<"5\n";break;
		case 1:cout<<"1\n";break;
		case 4:		
			if((b%10)%2==1)
			cout<<"4\n";
			else
			cout<<"6\n";
			break;
		case 9:	if((b%10)%2==1)
			cout<<"9\n";
			else
			cout<<"1\n";
			break;
		case 2: 
			switch(c%4)
			{
			case 0:	cout<<"6\n";
				break;
			case 1: cout<<"2\n";
				break;
			case 2: cout<<"4\n";
				break;
			case 3: cout<<"8\n";
				break;
			}
			break;
		case 3:
			switch(c%4)
			{
			case 0:	cout<<"1\n";
				break;
			case 1: cout<<"3\n";
				break;
			case 2: cout<<"9\n";
				break;
			case 3: cout<<"7\n";
				break;
			}
			break;
		case 7:
			switch(c%4)
			{
			case 0:	cout<<"1\n";
				break;
			case 1: cout<<"7\n";
				break;
			case 2: cout<<"9\n";
				break;
			case 3: cout<<"3\n";
				break;
			}
			break;
		case 8:						
			switch(c%4)
			{
			case 0:	cout<<"6\n";
				break;
			case 1: cout<<"8\n";
				break;
			case 2: cout<<"4\n";
				break;
			case 3: cout<<"2\n";
				break;
			}
			break;
	}
	}
	return 0;
}
