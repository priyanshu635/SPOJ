#include<iostream>

using namespace std;


int power(int a,unsigned long long int b)
{
	if(b==0)
		return 1;

	if(a==0)
		return 0;

	int answer=a;
	b--;
	while(b>0)
	{
	if(b%2!=0)
		answer=(answer*a)%10;
	a=(a*a)%10;
	b=b/2;
	}
	
	return answer%10;
}

int main()
{
	unsigned long long int b;
	int a,t,i;
	cin>>t;
	for(i=0;i<t;i++)
	{
	cin>>a>>b;
	cout<<power(a,b)<<"\n";
	}
	return 0;
}
